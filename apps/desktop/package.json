{
  "name": "desktop",
  "version": "0.1.0",
  "main": "dist/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite --config vite.main.config.ts",
    "dev:electron": "wait-on tcp:3000 && electron .",
    "build": "vite build --config vite.main.config.ts && vite build --config vite.preload.config.ts",
    "start": "electron .",
    "package": "electron-builder"
  },
  "devDependencies": {
    "electron": "^31.0.0",
    "electron-builder": "^24.14.1",
    "vite": "^5.2.0",
    "typescript": "^5.0.0",
    "cross-env": "^7.0.3",
    "wait-on": "^7.0.1"
  }
}